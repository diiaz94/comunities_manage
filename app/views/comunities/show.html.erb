<div id="notice" class="alert alert-success hidden" role="alert">
  <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
  <span class="sr-only"></span>
  <span class="cerrar" onclick="$('#notice').hide();">X</span>
  <%=notice%>
</div>
<div id="error" class="alert alert-danger hidden" role="alert">
  <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
  <span class="sr-only">Error:</span>
  <span class="cerrar" onclick="$('#error').hide();">X</span>
  <%=alert%>
</div>
<p>
  <strong>Estado:</strong>
  <%= @comunity.parish.town.state.nombre %>
</p>
<p>
  <strong>Municipio:</strong>
  <%= @comunity.parish.town.nombre %>
</p>

<p>
  <strong>Parroquia:</strong>
  <%= @comunity.parish.nombre %>
</p>

<p>
  <strong>Rif:</strong>
  <%= @comunity.rif_format %>
</p>
<p>
  <strong>Codigo de registro:</strong>
  <%= @comunity.cod_registro_format %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @comunity.nombre %>
</p>

<p>
  <strong>Nro Catastro:</strong>
  <%= @comunity.catastro_format %>
</p>
<p>
  <strong>Sector:</strong>
  <%= @comunity.sector %>
</p>
<p>
  <strong>Direccion:</strong>
  <%= @comunity.direccion %>
</p>


<% if session[:type_user]=="Administrador"%>
 	<button type="button" class="btn btn-danger" aria-label="Left Align" onclick="window.location.href='<%=comunities_path%>'">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true" ></span>
      Regresar &nbsp;
	</button>
    <button type="button" class="btn btn-primary" aria-label="Left Align" onclick="window.location.href='<%=edit_comunity_path(@comunity)%>'">
      <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
      Editar &nbsp;
    </button>
<%else%>
  <button type="button" class="btn btn-danger" aria-label="Left Align" onclick="window.location.href='<%=root_path%>'">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true" ></span>
      Regresar &nbsp;
  </button>
  <button type="button" class="btn btn-primary" aria-label="Left Align" onclick="window.location.href='<%=my_comunity_edit_path%>'">
      <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
      Editar &nbsp;
   </button>
<%end%>
